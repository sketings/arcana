{
  "compilerOptions": {
    "baseUrl": ".",
    "module": "nodenext",
    "target": "ESNext",
    "outDir": "./dist/",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
