{
  "compilerOptions": {
    "module": "ES2020",
    "baseUrl": ".",
    "target": "ES2020",
    "outDir": "./dist/",
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "moduleResolution": "classic"
  }
}
